{
  "name": "next-react-workbook",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "run-p 'lint:*'",
    "lint:eslint": "eslint '**/*.{ts,tsx}'",
    "lint:prettier": "prettier --check '**/*.{ts,tsx}'",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "vercel-build": "yum install libuuid-devel libmount-devel && cp /lib64/{libuuid,libmount,libblkid}.so.1 node_modules/canvas/build/Release/ && next build"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier",
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn tsc && yarn build"
    }
  },
  "dependencies": {
    "@guildex/react-next-curriculum-example": "^0.5.6",
    "@mdx-js/mdx": "^2.1.2",
    "@nextui-org/react": "^1.0.0-beta.9",
    "@react-aria/ssr": "^3.1.2",
    "canvas": "2.9.1",
    "dayjs": "^1.11.3",
    "mdx": "^0.3.1",
    "next": "12.1.5",
    "next-mdx-remote": "^4.0.3",
    "next-seo": "^5.4.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-google-font-loader": "^1.1.0",
    "react-icons": "^4.3.1",
    "react-use": "^17.4.0",
    "rehype-highlight": "^5.0.2",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@guildex/eslint-config": "^1.0.8",
    "@mdx-js/loader": "^2.1.2",
    "@mdx-js/react": "^2.1.2",
    "@next/mdx": "^12.2.2",
    "@types/node": "17.0.25",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/styled-components": "^5.1.25",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.18.0",
    "eslint-config-next": "12.1.5",
    "husky": "^8.0.1",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "typescript": "4.5.5"
  }
}
